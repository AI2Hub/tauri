{
  "name": "@tauri-apps/api",
  "version": "1.0.0-beta.8",
  "description": "Tauri API definitions",
  "type": "module",
  "funding": {
    "type": "opencollective",
    "url": "https://opencollective.com/tauri"
  },
  "files": [
    "CHANGELOG.md",
    "dist"
  ],
  "exports": {
    "app": {
      "import": "./dist/app.mjs",
      "require": "./dist/app.cjs"
    },
    "bundle": {
      "import": "./dist/bundle.mjs",
      "require": "./dist/bundle.cjs"
    },
    "cli": {
      "import": "./dist/cli.mjs",
      "require": "./dist/cli.cjs"
    },
    "clipboard": {
      "import": "./dist/clipboard.mjs",
      "require": "./dist/clipboard.cjs"
    },
    "dialog": {
      "import": "./dist/dialog.mjs",
      "require": "./dist/dialog.cjs"
    },
    "event": {
      "import": "./dist/event.mjs",
      "require": "./dist/event.cjs"
    },
    "fs": {
      "import": "./dist/fs.mjs",
      "require": "./dist/fs.cjs"
    },
    "globalShortcut": {
      "import": "./dist/globalShortcut.mjs",
      "require": "./dist/globalShortcut.cjs"
    },
    "http": {
      "import": "./dist/http.mjs",
      "require": "./dist/http.cjs"
    },
    "index": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "notification": {
      "import": "./dist/notification.mjs",
      "require": "./dist/notification.cjs"
    },
    "os": {
      "import": "./dist/os.mjs",
      "require": "./dist/os.cjs"
    },
    "path": {
      "import": "./dist/path.mjs",
      "require": "./dist/path.cjs"
    },
    "process": {
      "import": "./dist/process.mjs",
      "require": "./dist/process.cjs"
    },
    "shell": {
      "import": "./dist/shell.mjs",
      "require": "./dist/shell.cjs"
    },
    "tauri": {
      "import": "./dist/tauri.mjs",
      "require": "./dist/tauri.cjs"
    },
    "updater": {
      "import": "./dist/updater.mjs",
      "require": "./dist/updater.cjs"
    },
    "window": {
      "import": "./dist/window.mjs",
      "require": "./dist/window.cjs"
    }
  },
  "scripts": {
    "build": "rimraf ./dist && rollup -c --silent && node ./scripts/after-build.mjs",
    "npm-pack": "yarn build && cd ./dist && npm pack",
    "npm-publish": "yarn build && cd ./dist && yarn publish --access public --loglevel silly",
    "lint": "eslint --ext ts \"./src/**/*.ts\"",
    "lint-fix": "eslint --fix --ext ts \"./src/**/*.ts\"",
    "lint:lockfile": "lockfile-lint --path yarn.lock --type yarn --validate-https --allowed-hosts npm yarn",
    "format": "prettier --write --end-of-line=auto \"./**/*.{cjs,js,jsx,ts,tsx,html,css,json}\" --ignore-path .gitignore",
    "format:check": "prettier --check --end-of-line=auto \"./**/*.{cjs,js,jsx,ts,tsx,html,css,json}\" --ignore-path .gitignore",
    "generate-docs": "typedoc src --githubPages false --readme none --entryDocument index.md --hideInPageTOC true"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tauri-apps/tauri.git"
  },
  "contributors": [
    "Tauri Programme within The Commons Conservancy"
  ],
  "license": "Apache-2.0 OR MIT",
  "bugs": {
    "url": "https://github.com/tauri-apps/tauri/issues"
  },
  "homepage": "https://github.com/tauri-apps/tauri#readme",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">= 12.13.0",
    "npm": ">= 6.6.0",
    "yarn": ">= 1.19.1"
  },
  "devDependencies": {
    "@babel/core": "7.16.7",
    "@babel/preset-env": "7.16.8",
    "@babel/preset-typescript": "7.16.7",
    "@rollup/plugin-babel": "5.3.0",
    "@rollup/plugin-commonjs": "21.0.1",
    "@rollup/plugin-node-resolve": "13.1.3",
    "@rollup/plugin-sucrase": "4.0.2",
    "@rollup/plugin-typescript": "8.3.0",
    "@typescript-eslint/eslint-plugin": "5.9.1",
    "@typescript-eslint/parser": "5.9.1",
    "eslint": "8.6.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-config-standard-with-typescript": "21.0.1",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "6.0.0",
    "eslint-plugin-security": "1.4.0",
    "fast-glob": "^3.2.11",
    "lockfile-lint": "4.6.2",
    "prettier": "2.5.1",
    "rimraf": "3.0.2",
    "rollup": "2.63.0",
    "rollup-plugin-terser": "7.0.2",
    "tslib": "2.3.1",
    "typedoc": "0.22.10",
    "typedoc-plugin-markdown": "3.11.11",
    "typescript": "4.5.4"
  },
  "dependencies": {
    "type-fest": "2.9.0"
  }
}